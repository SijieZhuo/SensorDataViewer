<Window x:Class="stressProject.RecordPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:stressProject"
        mc:Ignorable="d"
        Title="RecordPopup" Height="150" Width="200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="21*"/>
            <ColumnDefinition Width="173*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="Save File as :" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="158" Height="28" Grid.ColumnSpan="2"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,43,0,0" TextWrapping="Wrap" Text="output" VerticalAlignment="Top" Width="162" Grid.ColumnSpan="2" TextChanged="textBox_TextChanged"/>
        <Button x:Name="BtnSave" Content="Save" HorizontalAlignment="Left" Margin="10,91,0,0" VerticalAlignment="Top" Width="75" Grid.ColumnSpan="2" Click="Save_Click"/>
        <Button x:Name="BtnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="87.333,91,0,0" VerticalAlignment="Top" Width="75" Grid.Column="1" Click="Cancel_Click"/>

    </Grid>
</Window>
